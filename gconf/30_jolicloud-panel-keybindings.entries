<gconfentryfile>
  <entrylist base="/apps/metacity/global_keybindings">
    <entry>
      <key>show_desktop</key>
      <value>
        <string>Super_L</string>
      </value>
    </entry>
    <entry>
      <key>panel_main_menu</key>
      <value>
        <string>&lt;Control&gt;Escape</string>
      </value>
    </entry>
    <entry>
      <key>run_command_terminal</key>
      <value>
        <string>&lt;Alt&gt;F1</string>
      </value>
    </entry>
  </entrylist>
</gconfentryfile>
